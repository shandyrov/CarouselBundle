<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- ... -->
        <service id="sputnik.theme_manager" class="App\Service\ThemeManager"  autowire="true"></service>

        <service id="sputnik_controller" class="App\Service\ThemeManager" autowire="true">
            <tag name="service" id="sputnik.theme_manager"/>
        </service>
               
        <service id="sputnik_carousel.routing_loader" class="Sputnik\CarouselBundle\Routing\ExtraLoader">
            <tag name="routing.loader"/>
        </service>
<!-- <service id="sputnik_controller" class="Sputnik\CarouselBundle\Controller\GeneralController"></service> -->
    </services>
</container>
